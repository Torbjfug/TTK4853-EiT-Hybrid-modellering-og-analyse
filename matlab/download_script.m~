years = [2017,2018,2019]; 
days = [5,15];


for year = 2018:2019
    for month = 1:12
        for day_index = 1:2
            
            day = days(day_index);
            month
            get_data_day(year,month,day);
        end
    end
end

for month = 8:12
    for day_index = 1:2
        day = days(day_index);
        month
        get_data_day(2017,month,day);
    end
end
